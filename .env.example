# CUPS Admin Configuration
CUPSADMIN=admin
CUPSPASSWORD=changeme

# Optional: Brother Driver Installation
# Set to "true" to install Brother printer drivers at runtime
INSTALL_BROTHER_DRIVER=false

# Brother Printer Model (if using Brother drivers)
# Examples: HL-1210W, HL-1212W, HL-2270DW, DCP-L2540DW
BROTHER_PRINTER_MODEL=HL-1210W

# Brother Installer Version
BROTHER_INSTALLER_VERSION=2.2.6-0

# Optional: HP Driver Installation
# Set to "true" to install HP printer drivers (HPLIP) at runtime
INSTALL_HP_DRIVER=false

# Timezone
TZ=UTC

# Optional: Skip USB device validation during startup
SKIP_USB_CHECK=false
